{
  "name": "game-rendering-engine",
  "version": "1.0.0",
  "description": "An isometric game rendering engine",
  "keywords": [
    "isometric"
  ],
  "homepage": "https://github.com/paulbjensen/game-rendering-engine#readme",
  "bugs": {
    "url": "https://github.com/paulbjensen/game-rendering-engine/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulbjensen/game-rendering-engine.git"
  },
  "license": "MIT",
  "author": "Paul Jensen",
  "type": "module",
  "scripts": {
    "analyze": "size-limit --why",
    "dev": "vite",
    "build": "vite build",
    "format": "npx @biomejs/biome format --write src",
    "lint": "npx @biomejs/biome check src",
    "preview": "vite preview",
    "size": "size-limit",
    "prepare": "husky",
    "test": "vitest --run"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.3",
    "@size-limit/esbuild-why": "^11.2.0",
    "@size-limit/file": "^11.2.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "size-limit": "^11.2.0",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@anephenix/event-emitter": "^0.0.10",
    "@sveu/browser": "^1.0.1"
  },
  "size-limit": [
    {
      "path": "dist/**/*.js",
      "brotli": true,
      "limit": "100 KB"
    }
  ]
}
